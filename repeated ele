#include <iostream>
using namespace std;

int main() {
    int a[]={1,2,1,2};
    int indxarr[5]={0};
    for(int i=0;i<4;i++)
    {
        int indx=a[i];
        indxarr[indx]+=1;
    }
    for(int i=0;i<4;i++)
    {
        if(indxarr[i]>1)
        cout<<"element repeated"<<i<<endl;
       
    }
    
	// your code goes hereint 
	return 0;
}
