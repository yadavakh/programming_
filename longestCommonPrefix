#include "bits/stdc++.h"
using namespace std;

string longestscommonprefix(vector<string>&v)
{
    sort(v.begin(),v.end());
    string f=v[0];
    string e=v[v.size()-1];
    int i=0;
    int j=0;
    string ans="";
    while(i<f.length() && j<e.length())
    {
     if(f[i]==e[j])
     ans+=f[i];
     else
     ans+="";
     i++;
     
    }
    return ans;
}

int main()
{
    int n;
    cin>>n;
    vector<string> v(n);
    for(int i=0;i<n;i++)
    {
        string s;
        cin>>s;
        v[i]=s;
    }
    string ans=longestscommonprefix(v);
    cout<<ans;
    return 0;
}
