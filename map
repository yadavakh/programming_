#include "bits/stdc++.h"
using namespace std;

int main()
{
    map <int,string> m;
    m[1]="abc";
    m[2]="bdx";
    m.insert({3,"endg"});
    m.insert({4,"afjh"});
    
    //  map <int,string> :: iterator it;
    //  for (it=m.begin();it!=m.end();++it)
    //  {
    //      cout<<(*it).first<<" "<<(*it).second<<endl;
    //  }
    for (auto &pr : m){
        cout<<pr.first<<" "<<pr.second<<endl;
    }
    auto it=m.find(3);
    if(it==m.end())
    {
      cout<<"no value";
    }
    else{
    cout<<(*it).first<<" "<<(*it).second<<endl;
    }
    
}
