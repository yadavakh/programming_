#include<bits/stdc++.h>
using namespace std;

int main()
{
    int a[]={7,1,5,3,6,4};
    int min= 8888;
    int j=0;
    for(int i=0;i<6;i++)
    {
        if(a[i]<min)
        min=a[i];
        
    }
    for (int i = 0; i < 6; ++i)
    {
        if (a[i] == min) j= i;
    }
     int maxdiff=-8989;
     int currentdiff=0;
    for(int i=j;i<6;i++)
    {
        if(a[i]-min>maxdiff)
        maxdiff=a[i]-min;
    }
    cout<<maxdiff;
    
    return 0;
}
